{
  "name": "wizardpig-2kpaid-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env-shell NODE_ENV=production babel-node -r module-alias/register ./bin/www | cross-env-shell NODE_ENV=production rotate-logs --config=$INIT_CWD/configs/app-log.config.js | cross-env-shell NODE_ENV=production rotate-logs --config=$INIT_CWD/configs/http-request-log.config.js",
    "dev": "cross-env-shell NODE_ENV=development nodemon --exec babel-node -r module-alias/register ./bin/www | cross-env-shell NODE_ENV=development rotate-logs --config=$INIT_CWD/configs/app-log.config.js | cross-env-shell NODE_ENV=development rotate-logs --config=$INIT_CWD/configs/http-request-log.config.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "db": "db-migrate --config '$INIT_CWD\\db\\dbmrc.json' -m '$INIT_CWD\\db\\migrations'",
    "db:create": "npm run db db:create",
    "db:drop": "npm run db db:drop",
    "migrate:up": "npm run db up:migrate",
    "migrate:down": "npm run db down:migrate",
    "migrate:create": "npm run db create:migrate",
    "migrate:reset": "npm run db reset:migrate",
    "seed:up": "npm run db up:seed",
    "seed:down": "npm run db down:seed",
    "seed:create": "npm run db create:seed"
  },
  "dependencies": {
    "@vrbo/pino-rotating-file": "^3.0.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.12",
    "db-migrate-mysql": "^2.1.2",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "dotenv-defaults": "^2.0.2",
    "express": "~4.16.1",
    "express-pino-logger": "^5.0.0",
    "helmet": "^4.6.0",
    "http-errors": "~1.6.3",
    "http-status-codes": "^2.1.4",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "nodemailer": "^6.4.11",
    "openapi-types": "^7.0.1",
    "pino": "^6.5.1",
    "randomatic": "^3.1.1",
    "swagger-ui-express": "^4.1.4",
    "util": "^0.12.2",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.15",
    "@babel/register": "^7.13.14",
    "cross-env": "^7.0.2",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-vipul-base": "^1.1.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.4",
    "pino-pretty": "^5.1.1",
    "prettier": "^2.2.1"
  },
  "_moduleAliases": {
    "@": ".",
    "@configs": "configs",
    "@constants": "constants",
    "@helpers": "helpers",
    "@middlewares": "middlewares",
    "@utils": "utils"
  }
}
