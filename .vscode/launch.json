{
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch via NPM",
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run-script", "debug"],
			"port": 3000,
			"resolveSourceMapLocations": ["!**/node_modules/**", "!node:internal/**"],
			"env": {
				"NODE_ENV": "development"
			},
			"envFile": "${workspaceFolder}\\.env.local"
		}
	]
}
